<div class="col-12">
    <h4><span class="icon-outside"><i class="social-foundicon-rss"></i></span> RSS Feeds Administration</h4>
</div>
<div class="col-12">
    <div class="outline clearfix">
        <table class="table table-striped">
            <thead>
                <tr>
                    <th>Name</th>
                    <th>Database</th>
                    <th>Feed URL</th>
                </tr>
            </thead>

            <tbody>
            {{#each feeds}}
                <tr>
                    <td>{{name}}</td>
                    <td>{{info.db}}</td>
                    <td>{{info.feed}}</td>
            {{/each}}
            </tbody>
        </table>
    </div>
</div>